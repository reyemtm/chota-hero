<div class="row">
  <% page.posts.each(function(post, i){ %>
  <div class="col-6 col-6-md col-4-lg" >
    <div class="card">
      <header>
        <figure style="background-image: url('/assets/img/<%- post.img %>');")>        </figure>
        <h3><%- post.title %></h3>

      </header>
      <section>
        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et
          dolore
          magna aliqua. Ut enim ad minim veniam, quis nostrud exercit.</p>
      </section>
      <footer class="">
        <a href="<%- post.permalink %>" class="button primary outline">Read More</a>
      </footer>
    </div>
  </div>
  <!-- <% if ((i === 2) || (i === 5)) {%><div class="is-full-width"></div><%}%> -->
  <%})%>
</div>
