<div class="row">
  <% page.posts.each(function(post, i){ %>
    <% var content = strip_html(post.content) %>
  <div class="col-6 col-6-md col-4-lg" >
    <div class="card">
      <header>
        <a href="<%- post.permalink %>"><figure style="background-image: url('/assets/img/<%- post.img %>');")></figure></a>
        <!-- <h3><%- post.title %></h3>
        <h5><%- post.subtitle %></h5>
        <%- partial('post/date', {post: post, class_name: 'article-date', date_format: null}) %> -->
        <%- partial('post/header', {post: post, index: true}) %>
      </header>
      <!-- <section><%- truncate(content, {length: 200, separator: ' '}) %></section> -->
      <footer class="">
        <a href="<%- post.permalink %>" class="button primary outline">Read More</a>
      </footer>
    </div>
  </div>
  <!-- <% if ((i === 2) || (i === 5)) {%><div class="is-full-width"></div><%}%> -->
  <%})%>
  <div class="is-full-width">
    &nbsp;
  </div>
  <div class="col">
    <a href="/blog" class="zp-headline zp-headline-dashed"><span>Older Posts</span></a>
  </div>
</div>
